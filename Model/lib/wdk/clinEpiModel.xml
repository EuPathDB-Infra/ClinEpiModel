<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE xml [
  <!ENTITY nbsp "&#160;">
  <!ENTITY lt   "&#60;">
  <!ENTITY gt   "&#62;">
]>

<wdkModel>

  <constant name="releaseDate">31 March 2017 00:00</constant>
  <constant name="buildNumber">1</constant>
  <constant name="releaseVersion">1</constant>

  <constant name="attributesReporterDisplayName">Tab delimited (Excel) - choose columns to make a custom table</constant>
  <constant name="tableReporterDisplayName">Tab delimited (Excel) - choose a pre-configured table</constant>
  <constant name="fullReporterDisplayName">Text: choose from columns and/or tables</constant>

  <modelName displayName="ClinEpiDB" version="%%releaseVersion%%" releaseDate="%%releaseDate%%" buildNumber="%%buildNumber%%" />

  <exampleStratsAuthor name="ClinEpiDB Example"/>

  <declaredMacro name="WEBAPP_BASE_URL"          usedBy="model"/>
  <declaredMacro name="LEGACY_WEBAPP_BASE_URL"   usedBy="model"/>

  <ontology name="Categories" pluginClass="org.gusdb.wdk.model.ontology.TestOntologyPlugin" useAsWdkCategories="true" />

    <import file="clinEpiModel/stepAnalysisPlugins.xml"/>

      <!-- Person record  -->
    <import file="clinEpiModel/records/participantRecord.xml"/>
    <import file="clinEpiModel/questions/queries/participantQueries.xml"/>
    <import file="clinEpiModel/questions/participantQuestions.xml"/> 
    <import file="clinEpiModel/questions/params/participantParams.xml"/>

    <!-- Dwelling record -->
    <import file="clinEpiModel/records/dwellingRecord.xml"/>
    <import file="clinEpiModel/questions/queries/dwellingQueries.xml"/>
    <import file="clinEpiModel/questions/dwellingQuestions.xml"/> 
    <import file="clinEpiModel/questions/params/dwellingParams.xml"/>

    <!-- Light Trap record -->
    <import file="clinEpiModel/records/lightTrapRecord.xml"/>
    <import file="clinEpiModel/questions/queries/mosquitoeQueries.xml"/>
    <import file="clinEpiModel/questions/mosquitoeQuestions.xml"/> 
    <import file="clinEpiModel/questions/params/mosquitoParams.xml"/>
    <import file="clinEpiModel/questions/params/lighttrapParams.xml"/>


    <!-- Clinical Visits record -->
    <import file="clinEpiModel/records/clinicalVisitRecord.xml"/>
    <import file="clinEpiModel/questions/queries/clinicalVisitQueries.xml"/>
    <import file="clinEpiModel/questions/clinicalVisitQuestions.xml"/> 
    <import file="clinEpiModel/questions/params/clinicalVisitParams.xml"/>

</wdkModel>
