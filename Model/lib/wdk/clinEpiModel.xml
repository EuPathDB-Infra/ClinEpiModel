<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE xml [
  <!ENTITY nbsp "&#160;">
  <!ENTITY lt   "&#60;">
  <!ENTITY gt   "&#62;">
]>

<wdkModel>

  <constant name="releaseDate">31 March 2017 00:00</constant>
  <constant name="buildNumber">1</constant>
  <constant name="releaseVersion">1</constant>

  <constant name="attributesReporterDisplayName">Tab delimited (Excel) - choose columns to make a custom table</constant>
  <constant name="tableReporterDisplayName">Tab delimited (Excel) - choose a pre-configured table</constant>
  <constant name="fullReporterDisplayName">Text: choose from columns and/or tables</constant>

  <modelName displayName="ClinEpiDB" version="%%releaseVersion%%" releaseDate="%%releaseDate%%" buildNumber="%%buildNumber%%" />

  <exampleStratsAuthor name="ClinEpiDB Example"/>

  <declaredMacro name="WEBAPP_BASE_URL"          usedBy="model"/>
  <declaredMacro name="LEGACY_WEBAPP_BASE_URL"   usedBy="model"/>

  <uiConfig showStratPanelByDefault="false"/>


    <!-- Categories -->
    <import file="model/questions/categories.xml"/>

    <import file="ebrcModelCommon.xml"/>

    <import file="model/stepAnalysisPlugins.xml"/>

      <!-- Person record  -->
    <import file="model/records/participantRecord.xml"/>
    <import file="model/questions/queries/participantQueries.xml"/>
    <import file="model/questions/participantQuestions.xml"/> 
    <import file="model/questions/params/participantParams.xml"/>

    <!-- Dwelling record -->
    <import file="model/records/dwellingRecord.xml"/>
    <import file="model/questions/queries/dwellingQueries.xml"/>
    <import file="model/questions/dwellingQuestions.xml"/> 
    <import file="model/questions/params/dwellingParams.xml"/>

    <!-- Light Trap record -->
    <import file="model/records/lightTrapRecord.xml"/>
    <import file="model/questions/queries/lightTrapQueries.xml"/>
    <import file="model/questions/lightTrapQuestions.xml"/> 
    <import file="model/questions/params/lightTrapParams.xml"/>


    <!-- Clinical Visits record -->
    <import file="model/records/clinicalVisitRecord.xml"/>
    <import file="model/questions/queries/clinicalVisitQueries.xml"/>
    <import file="model/questions/clinicalVisitQuestions.xml"/> 
    <import file="model/questions/params/clinicalVisitParams.xml"/>

</wdkModel>
